{
  "server": {
    "host": "dev1.web.michaels.demandware.net",
    "basePath": "on/demandware.store",
    "site": "Sites-beyond-Site",
    "panel": "Sites-Site",
    "defaultLocale": "en",
    "noLocale": "default"
  },

  "repos": {
    "sitegenesis": {
      "clone": "https://github.com/FluidInc/demandware-sitegenesis.git",
      "branch": "release/13.5",
      "directory": "sitegenesis"
    },
    "fluid": {
      "clone": "https://github.com/FluidInc/demandware-sitegenesis-fluid.git",
      "branch": "master",
      "directory": "fluidstorefront"
    }
  },

  "cartridges": {
    "core": "sitegenesis/cartridges/app_storefront_core/cartridge",
    "richUI": "sitegenesis/cartridges/app_storefront_richUI/cartridge",
    "fluid": "fluidstorefront/cartridges/app_fluid/cartridge",
    "project": "cartridges/app_michaels/cartridge"
  },

  "path": {
    "scssDir": "static/default/scss",
    "cssDir": "static/default/css",
    "imagesDir": "static/default/images",
    "scriptDir": "static/default/script",
    "fontsDir": "static/default/fonts"
  }
}
